{
  "name": "east",
  "version": "0.4.0",
  "description": "east (edit AST) aims to be a code editor, that works on an AST instead of textual code. The first AST will be an ECMAScript compatible AST.",
  "main": "./out/static/js/main.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf ./out & webpack --config ./build-scripts/webpack.config.js",
    "start": "electron ."
  },
  "keywords": [
    "ast",
    "editor",
    "ecmascript"
  ],
  "author": "Sven Busse",
  "license": "MIT",
  "dependencies": {
    "esprima": "^4.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/esprima": "^4.0.1",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.93",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "css-loader": "^0.28.9",
    "electron": "^1.7.11",
    "electron-devtools-installer": "^2.2.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "node-sass": "^4.7.1",
    "postcss": "^6.0.16",
    "postcss-loader": "^2.0.10",
    "postcss-modules-extract-imports": "^1.1.0",
    "postcss-modules-local-by-default": "^1.2.0",
    "postcss-modules-scope": "^1.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.3.1",
    "typescript": "^2.6.2",
    "typings-for-css-modules-loader": "^1.5.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-stats-plugin": "^0.1.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
